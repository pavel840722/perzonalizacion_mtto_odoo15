<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_df_work_order_product_tree" model="ir.ui.view">
            <field name="name">df.work.order.product.tree</field>
            <field name="model">df.work.order.product</field>
            <field eval="100" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Work Order Product">
                    <field name="print_validate"/>
                    <field name="product_id" />
                    <field name="activity_id" />
                    <field name="product_qty" />
                    <field name="product_uom"/>
                    <field name="price_unit"/>
                    <field name="amount"/>
                    <field name="location" />
                    <field name="store_id"/>
                    <field name="date"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>